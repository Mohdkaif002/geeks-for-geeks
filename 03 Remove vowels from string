class Solution {
    String removeVowels(String s) {
        char[] arr = s.toCharArray();
        StringBuilder ans = new StringBuilder();
        
        for (char c : arr) {
            if (!(c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||
                  c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U')) {
                ans.append(c);
            }
        }
        
        return ans.toString();
    }
}
